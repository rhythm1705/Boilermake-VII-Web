		"body-parser": "^1.19.0",
		"concurrently": "^5.0.2",
		"cookie-parser": "~1.4.4",
		"cors": "^2.8.5",
		"debug": "~2.6.9",
		"dotenv": "^8.2.0",
		"express": "^4.16.4",
		"http-errors": "~1.6.3",
		"is-empty": "^1.2.0",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^5.8.9",
		"morgan": "~1.9.1",
		"passport": "^0.4.1",
		"passport-jwt": "^4.0.0",
		"pug": "2.0.0-beta11",
		"validator": "^13.7.0"
	}
}
